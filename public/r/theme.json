{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "theme",
  "type": "registry:theme",
  "cssVars": {
    "theme": {
      "font": "var(--font-rubik)"
    },
    "light": {
      "primary": "oklch(0.45 0.25 250)", 
      "secondary": "oklch(0.85 0.15 80)",
      "tertiary": "oklch(0.15 0 0)",
      "secondary-foreground": "oklch(0.45 0.25 250)",
      "border": "oklch(0.85 0.15 80)"
    },
    "dark": {
      "primary": "oklch(0.45 0.25 250)",
      "secondary": "oklch(0.85 0.15 80)",
      "tertiary": "oklch(0.15 0 0)",
      "secondary-foreground": "oklch(0.45 0.25 250)",
      "border": "oklch(0.85 0.15 80)"
    }
  },
  "css": {
    "@layer base": {
      "body": {
        "font-family": "var(--font-rubik)"
      }
    }
  }
}